{
    "name": "loop-vscode",
    "displayName": "loop-vscode",
    "description": "Syntax highlighting for the Loop programming language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "loop",
            "aliases": ["Loop", "loop"],
            "extensions": [".lp",".loop"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "loop",
            "scopeName": "source.loop",
            "path": "./syntaxes/loop.tmLanguage.json"
        }]
    }
}